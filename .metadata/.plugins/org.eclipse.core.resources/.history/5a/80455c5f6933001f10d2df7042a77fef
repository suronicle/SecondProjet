<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-2-mapper.dtd" >
<mapper namespace="com.example.demo.dao.MovieDao">
    <insert id="movieInsert" parameterType="MovieDTO">
        INSERT INTO movietbl
        VALUES (null,#{m_director},#{m_name},#{m_actor},
                #{m_genre},#{m_nation},#{m_open},
                #{m_synopsis},#{p_oriname},#{p_sysname})
    </insert>
    <update id="movieUpdate" parameterType="MovieDTO">
        UPDATE movietbl
        SET m_name=#{m_name},m_director=#{m_director},m_nation=#{m_nation},
            m_genre=#{m_genre},m_actor=#{m_actor},m_open=#{m_open},
            m_synopsis=#{m_synopsis},p_oriname=#{p_oriname},p_sysname=#{p_sysname}
        WHERE m_code=#{m_code}
    </update>

    <select id="getMovieList" resultType="MovieDTO">
        SELECT * FROM movietbl
    </select>
</mapper>