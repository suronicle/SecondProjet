package kr.co.book.ecom.controller;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.GetMapping;

@Controller
public class jeoncontroller {

	@GetMapping("/")
	   public String elogin(){
	      return "e_login";
	   }
	
	@GetMapping("/join")
	   public String join(){
	      return "e_join";
	   }
	
}
